Encoding: UTF-8
LazyData: true
RoxygenNote: 7.1.1
Depends: 
    dplyr,
    R (>= 2.10)
Imports:
    ggplot2,
	tidyverse,
	magrittr,
	janitor,
	gt,
	zoo,
	data.table,
	usethis
URL: https://faunalytics.github.io/faunalytics/
